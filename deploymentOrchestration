pipeline {
    agent any
    stages {
    	stage('preparation') {
	    	parallel {
			    stage('assigned release specification') {
			    	steps {
			    		sleep(time:5,unit:"SECONDS")
			    	}
			    }	    
			    stage('config data validation status') {
			    	steps {
			    		sleep(time:5,unit:"SECONDS")
			    	}
			    }
			    stage('fetch environment settings') {
			    	steps 
			    		{sleep(time:5,unit:"SECONDS")
			    	}
			    }
	    	}//parallel
	    }//preparation
	    stage('preDeploy verification') {
		    stage('collect current settings') {
		    	steps {
		    		sleep(time:5,unit:"SECONDS")
		    	}
		    }	    
		    stage('check for unauthorized changes') {
		    	steps {
		    		sleep(time:5,unit:"SECONDS")
		    	}
		    }
		    stage('perform sanityCheck') {
		    	steps 
		    		{sleep(time:5,unit:"SECONDS")
		    	}
		    }

	    }//predeployVerification
    }//Stages
 }//Pipeline